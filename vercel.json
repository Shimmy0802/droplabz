{
    "buildCommand": "cd apps/web && pnpm build",
    "devCommand": "cd apps/web && pnpm dev",
    "installCommand": "pnpm install",
    "framework": "nextjs",
    "outputDirectory": "apps/web/.next",
    "cleanUrls": true,
    "trailingSlash": false,
    "env": {
        "DATABASE_URL": "@DATABASE_URL",
        "NEXTAUTH_SECRET": "@NEXTAUTH_SECRET",
        "NEXTAUTH_URL": "@NEXTAUTH_URL",
        "DISCORD_CLIENT_ID": "@DISCORD_CLIENT_ID",
        "DISCORD_CLIENT_SECRET": "@DISCORD_CLIENT_SECRET",
        "DISCORD_BOT_TOKEN": "@DISCORD_BOT_TOKEN",
        "NEXT_PUBLIC_DISCORD_TICKET_URL": "@NEXT_PUBLIC_DISCORD_TICKET_URL",
        "SUPER_ADMIN_DISCORD_IDS": "@SUPER_ADMIN_DISCORD_IDS",
        "SUPER_ADMIN_WALLET_ADDRESSES": "@SUPER_ADMIN_WALLET_ADDRESSES",
        "SOLANA_RPC_URL": "@SOLANA_RPC_URL",
        "NEXT_PUBLIC_SOLANA_NETWORK": "@NEXT_PUBLIC_SOLANA_NETWORK",
        "NEXT_PUBLIC_SOLANA_RPC_URL": "@NEXT_PUBLIC_SOLANA_RPC_URL",
        "SOLANA_PROGRAM_ID": "@SOLANA_PROGRAM_ID",
        "APP_BASE_URL": "@APP_BASE_URL"
    },
    "build": {
        "env": {
            "SKIP_ENV_VALIDATION": "1"
        }
    }
}
